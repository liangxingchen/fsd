{
  "name": "fsd-dev",
  "version": "0.1.0",
  "description": "General file system driver for Node.js",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "test": "cross-env NODE_ENV=test tape test/ts.js",
    "cover": "cross-env NODE_ENV=test nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "prettier": "prettier -l \"./**/*\"",
    "prettier:fix": "prettier --write -l \"./**/*\"",
    "fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangxingchen/fsd.git"
  },
  "author": "Liang <liang@miaomo.cn> (https://github.com/liangxingchen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liangxingchen/fsd/issues"
  },
  "homepage": "https://github.com/liangxingchen/fsd#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "test",
      "**/lib/**",
      ".history/",
      ".*",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "resolutions": {
    "globals": "15.13.0",
    "whatwg-url": "14.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/async": "^3.2.25",
    "@types/crypto-js": "4.1.1",
    "@types/debug": "0.0.31",
    "@types/delay": "^3.1.3",
    "@types/is-stream": "^2.0.0",
    "@types/lodash": "^4.17.21",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.10.1",
    "@types/qs": "^6.14.0",
    "@types/tape": "^5.8.1",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^10.1.0",
    "delay": "^5.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "glob": "^10.5.0",
    "lerna": "^9.0.3",
    "lodash": "^4.17.21",
    "lru-cache": "^11.2.4",
    "mkdirp": "^3.0.1",
    "nyc": "^17.1.0",
    "prettier": "^3.7.4",
    "tape": "^5.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
