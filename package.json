{
  "name": "fsd-dev",
  "version": "0.1.0",
  "description": "General file system driver for Node.js",
  "main": "index.js",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "build": "lerna run build",
    "test": "cross-env NODE_ENV=test tape test/ts.js",
    "cover": "cross-env NODE_ENV=test nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts ./",
    "eslint:fix": "eslint --ext .ts --fix ./",
    "prettier": "prettier -l \"./**/*\"",
    "prettier:fix": "prettier --write -l \"./**/*\"",
    "fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/fsd.git"
  },
  "author": "Liang <liang@maichong.it> (https://github.com/liangxingchen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/fsd/issues"
  },
  "homepage": "https://github.com/maichong/fsd#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "test",
      "**/lib/**",
      ".history/",
      ".*",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/ali-oss": "^6.16.3",
    "@types/async": "^3.2.12",
    "@types/debug": "0.0.31",
    "@types/delay": "^3.1.0",
    "@types/glob": "^7.2.0",
    "@types/is-stream": "^2.0.0",
    "@types/lodash": "^4.14.181",
    "@types/lru-cache": "^5.1.1",
    "@types/mz": "0.0.32",
    "@types/node": "^17.0.23",
    "@types/qs": "^6.9.7",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "ali-oss": "^6.17.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "delay": "^5.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-config-prettier": "^8.5.0",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1",
    "tape": "^5.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
