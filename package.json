{
  "name": "fsd-dev",
  "version": "0.1.0",
  "description": "General file system driver for Node.js",
  "main": "index.js",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "test": "cross-env NODE_ENV=test tape test/ts.js",
    "cover": "cross-env NODE_ENV=test nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts ./",
    "eslint:fix": "eslint --ext .ts --fix ./",
    "prettier": "prettier -l \"./**/*\"",
    "prettier:fix": "prettier --write -l \"./**/*\"",
    "fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/fsd.git"
  },
  "author": "Liang <liang@maichong.it> (https://github.com/liangxingchen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/fsd/issues"
  },
  "homepage": "https://github.com/maichong/fsd#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "test",
      "**/lib/**",
      ".history/",
      ".*",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/ali-oss": "^6.0.5",
    "@types/async": "^3.2.0",
    "@types/debug": "0.0.31",
    "@types/delay": "^3.1.0",
    "@types/glob": "^7.1.1",
    "@types/is-stream": "^2.0.0",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "@types/mz": "0.0.32",
    "@types/node": "^13.11.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.6.0",
    "eslint-config-prettier": "^6.10.1",
    "lerna": "^3.20.2",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "tape": "^4.13.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
