[ignore]
.*/build/.*
.*/dest/.*
.*/node_modules/.*
.*/flow-declarations/.*
.*/test/.*
.*/lib/.*

[libs]
flow-declarations/async/
flow-declarations/ali-oss/
flow-declarations/co/
flow-declarations/cpr/
flow-declarations/debug/
flow-declarations/lodash/
flow-declarations/fsd/
flow-declarations/minimatch/
flow-declarations/glob/
flow-declarations/mz/
flow-declarations/rimraf/
flow-declarations/is-stream/

[options]
suppress_comment= \\(.\\|\n\\)*\\$Flow
esproposal.decorators=ignore
esproposal.export_star_as=enable
