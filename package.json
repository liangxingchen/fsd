{
  "name": "fsd-dev",
  "version": "0.1.0",
  "description": "General file system driver for Node.js",
  "main": "index.js",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "test": "cross-env NODE_ENV=test tape test/ts.js",
    "cover": "cross-env NODE_ENV=test nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts ./",
    "fix": "eslint --ext .ts --fix ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/fsd.git"
  },
  "author": "Liang <liang@maichong.it> (https://github.com/liangxingchen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/fsd/issues"
  },
  "homepage": "https://github.com/maichong/fsd#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "test",
      "**/lib/**",
      ".history/",
      ".*",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/ali-oss": "^6.0.4",
    "@types/async": "^3.0.1",
    "@types/debug": "0.0.31",
    "@types/delay": "^3.1.0",
    "@types/glob": "^7.1.1",
    "@types/is-stream": "^2.0.0",
    "@types/lodash": "^4.14.137",
    "@types/mz": "0.0.32",
    "@types/node": "^12.7.2",
    "@types/rimraf": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "del": "^5.0.0",
    "delay": "^4.3.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.2.1",
    "eslint-config-alloy": "^2.0.5",
    "lerna": "^3.16.4",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "nyc": "^14.1.1",
    "tape": "^4.11.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.3.3"
  }
}
