{
  "name": "fsd-dev",
  "version": "0.1.0",
  "description": "General file system driver for Node.js",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "test": "cross-env NODE_ENV=test tape test/ts.js",
    "cover": "cross-env NODE_ENV=test nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts ./",
    "eslint:fix": "eslint --ext .ts --fix ./",
    "prettier": "prettier -l \"./**/*\"",
    "prettier:fix": "prettier --write -l \"./**/*\"",
    "fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangxingchen/fsd.git"
  },
  "author": "Liang <liang@miaomo.cc> (https://github.com/liangxingchen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liangxingchen/fsd/issues"
  },
  "homepage": "https://github.com/liangxingchen/fsd#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "test",
      "**/lib/**",
      ".history/",
      ".*",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/async": "^3.2.21",
    "@types/crypto-js": "4.1.1",
    "@types/debug": "0.0.31",
    "@types/delay": "^3.1.0",
    "@types/glob": "^8.1.0",
    "@types/is-stream": "^2.0.0",
    "@types/lodash": "^4.14.199",
    "@types/mime-types": "^2.1.2",
    "@types/node": "^20.8.2",
    "@types/qs": "^6.9.8",
    "@types/tape": "^5.6.1",
    "@types/xml2js": "^0.4.12",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "delay": "^5.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.0.0",
    "lerna": "^7.3.0",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.1",
    "mkdirp": "^3.0.1",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "tape": "^5.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
