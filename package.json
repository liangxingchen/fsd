{
  "name": "fsd-dev",
  "version": "0.1.0",
  "description": "General file system driver for Node.js",
  "main": "index.js",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "test": "cross-env NODE_ENV=test tape test/ts.js",
    "cover": "cross-env NODE_ENV=test nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint ./",
    "fix": "eslint --fix ./",
    "flow": "flow check --show-all-errors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/fsd.git"
  },
  "author": "Liang <liang@maichong.it> (https://github.com/liangxingchen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/fsd/issues"
  },
  "homepage": "https://github.com/maichong/fsd#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "test",
      "**/lib/**",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.0",
    "@types/async": "^2.0.50",
    "@types/debug": "0.0.31",
    "@types/delay": "^3.1.0",
    "@types/glob": "^7.1.1",
    "@types/is-stream": "^1.1.0",
    "@types/lodash": "^4.14.117",
    "@types/mz": "0.0.32",
    "@types/node": "^10.12.1",
    "@types/rimraf": "^2.0.2",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "delay": "^4.1.0",
    "dotenv": "^6.1.0",
    "lerna": "^3.4.3",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "nyc": "^13.1.0",
    "tape": "^4.9.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.4"
  }
}
